
<form action="FormCreaClienteServlet">
	<input type="text" class="name" name="name" placeholder="Nombre y apellidos" style="background-color:transparent;color:black;">
	<input type="text" class="dni" name="dni" placeholder="DNI" style="background-color:transparent;color:black">
	<input type="text" class="email" name="email" placeholder="Email" style="background-color:transparent;color:black">
	<input type="text" class="tlf" name="tlf" placeholder="Telefono" style="background-color:transparent;color:black">
	<input type="text" class="room" name="room" placeholder="Habitacion" style="background-color:transparent;color:black">
	<input type="password" class="password" name="password" placeholder="Password" style="background-color:transparent;color:black">
	<button type="submit" class="botonRegistrar">Registrar cliente</button>
</form>

